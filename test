    BuildTemplate:

        runs-on: ubuntu-latest

        steps:
            - uses: actions/checkout@v2
            - uses: actions/setup-node@v2
            with:
                node-version: '12'
            - name: Change into Template
                run: cd ./Template
            - name: Install packages
                run: yarn install
            - name: Build App
                run: yarn build
            - name: Current directory
                run: pwd
            - uses: actions/upload-artifact@v2
            with:
                name: template-build
                path: ./build